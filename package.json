  "dependencies": {
    "chalk": "5.2.0",
    "commander": "10.0.0",
    "date-fns": "2.29.3",
    "figures": "5.0.0",
    "jsonfile": "6.1.0",
    "ws": "8.12.1"
  },
  "devDependencies": {
    "prettier": "2.8.4",
    "rimraf": "4.3.0",
    "sort-package-json": "2.4.1",
    "standard": "17.0.0",
    "upgradeps": "2.0.5"
  }
}
