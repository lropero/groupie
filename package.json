{
  "name": "groupie",
  "version": "1.0.0",
  "private": true,
  "description": "Binance grouped times & sales.",
  "license": "UNLICENSED",
  "author": "Luciano Ropero <lropero@gmail.com>",
  "type": "module",
  "scripts": {
    "audit": "npx upgradeps -v",
    "clean": "npx rimraf node_modules && npx rimraf package-lock.json",
